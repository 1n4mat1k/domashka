#include <stdio.h>

int main()
{
    int a, b, c;
    printf("Введите значение переменной A: ");
    scanf("%d", &a);
    printf("Введите значение переменной B: ");
    scanf("%d", &b);
    
    if (a != b){
        if (a < b){
            a = b;
            b = b;
            printf ("Новые значения перменных A и B соответсвенно: %d %d", a, b);
        }
        else if (b < a){
            a = a;
            b = a;
            printf ("Новые значения перменных A и B соответсвенно: %d %d", a, b);
        }
    }
    else if (a == b){
        a = 0;
        b = 0;
        printf("Новые значения перменных A и B соответсвенно: %d %d", a, b);
    }

    return 0;
}
