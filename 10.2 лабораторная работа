#include <stdio.h>

int main()
{
    float a, b, c;
    printf("Введите значение переменной A: ");
    scanf("%f", &a);
    printf("Введите значение переменной: B: ");
    scanf("%f", &b);
    printf("Введите значение переменной: C: ");
    scanf("%f", &c);
    
    if (a < b & b < c){
        printf("Двойное неравенство справедливо");
    }
    else{
       printf("Двойное неравенство несправедливо"); 
    }

    return 0;
}
