#include <stdio.h>
int main()
{
    float v1, v2, s, t, s1;
    printf("Введите значение скорости первого автомобиля: ");
    scanf("%f", &v1);
    printf("Введите значение скорости второго автомобиля: ");
    scanf("%f", &v2);
    printf("Введите значение количества часов: ");
    scanf("%f", &t);
    printf("Введите значение расстояния между ними: ");
    scanf("%f", &s);
    
    s1 = s + v1*t + v2*t;
    
    printf("Расстояние между ними через %.1f часов составляет %.2f км", t, s1);
     
    return 0;
}
