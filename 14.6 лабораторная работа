#include <stdio.h>

int main()
{
    int a, b, c, i, m, k;
    printf("Введите число Фибоначчи: ");
    scanf("%d", &k);
    a = 0;
    b = 1;
    m = 0;
    c = 0;
    if (k == 0){
        printf("Порядковый номер этого числа: 1");
    }
    if (k == 1){
        printf("Порядковый номер этого числа: 2 и 3");
    }
        else{
            while(c != k){
                c = a + b;
                a = b;
                b = c;
                m++;
            }
        m = 2 + m;
        printf("Порядковый номер этого числа: %d", m);
        }

    return 0;
}
