#include <stdio.h>

int main()
{
    float a, b;
    printf("Введите значение переменной A: ");
    scanf("%f", &a);
    printf("Введите значение переменной: B: ");
    scanf("%f", &b);
    
    if (a > 2 & b <= 3){
        printf("Эти неравенства справедливы");
    }
    else{
       printf("Эти неравенства несправедливы"); 
    }

    return 0;
}
