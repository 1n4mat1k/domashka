#include <stdio.h>

int main()
{
    int a;
    printf("Введите значение числа a: ");
    scanf("%d", &a);
    if (a % 2 == 0 & a / 100 == 0){
        printf("Данное число является чётным двузначным: ");
    }
    else{
        printf("Данное число не является чётным двузначным: ");
    }
    

    return 0;
}
