#include <stdio.h>
int main() {
    float n, a, b, i;
    float d = 1;
    float c = 1.3;
    b = 1.1 * 1.2 * 1.3;
    printf("Введите количество N сомножителей: ");
    scanf("%f", &n);
    for (i = 0; i < n; i++){
        c += 0.1;
        d = c * d; 
    }
    a = b * d;
    printf("Ответ: %f", a);
    
    return 0;
}
