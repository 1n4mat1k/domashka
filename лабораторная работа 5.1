#include <stdio.h>
#include <math.h>

int main()
{
    float x1, y1, x2 , y2, s;
    printf("Введите координаты первой точки (x1, y1): ");
    printf("x1 = ");
    scanf("%f/n", &x1);
    printf("y1 = ");
    scanf("%f", &y1);
    printf("Введите координаты первой точки (x2, y2): ");
    printf("x2 = ");
    scanf("%f/n", &x2);
    printf("y2 = ");
    scanf("%f", &y2);
    s = sqrt((x2-x1)*(x2-x1)+(y2-y1)*(y2-y1));
    printf("%f", s);

    return 0;
}
