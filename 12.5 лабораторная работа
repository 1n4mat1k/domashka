#include <stdio.h>
 
int main(void)
{
   int n = 4, i, a;
   printf("Введите год: ") ;
   scanf ("%i", &i);
   printf("Год ");
   a = i - n;
   switch (a % 25){
       case 0:
       case 1:
       case 2:
       case 3:
       case 4:
       case 5:
       case 6:
       case 7:
       case 8:
       case 9:
       case 10:
       case 11:
           printf ("зелёной ");
           break;
           
       case 12:
       case 13:
       case 14:
       case 15:
       case 16:
       case 17:
       case 18:
       case 19:
       case 20:
       case 21:
       case 22:
       case 23:
           printf ("красной ");
           break;
           
       case 24:
       case 25:
       case 26:
       case 27:
       case 28:
       case 29:
       case 30:
       case 31:
       case 32:
       case 33:
       case 34:
       case 35:
           printf ("жёлтой ");
           break;
           
       case 36:
       case 37:
       case 38:
       case 39:
       case 40:
       case 41:
       case 42:
       case 43:
       case 44:
       case 45:
       case 46:
       case 47:
           printf ("белой ");
           break;
           
       case 48:
       case 49:
       case 50:
       case 51:
       case 52:
       case 53:
       case 54:
       case 55:
       case 56:
       case 57:
       case 58:
       case 59:
           printf ("чёрной ");
           break;
   }
   
   switch (a % 12){
       case 0:
           printf ("крысы ");
           break;
           
       case 1:
           printf ("коровы ");
           break;
           
       case 2:
           printf ("тигра ");
           break;
           
       case 3:
           printf ("зайца ");
           break;
           
       case 4:
           printf ("дракона ");
           break;
           
       case 5:
           printf ("змеи ");
           break;
           
       case 6:
           printf ("лошади ");
           break;
           
       case 7:
           printf ("овцы ");
           break;
           
       case 8:
           printf ("обезьяны ");
           break;
           
       case 9:
           printf ("курицы ");
           break;
           
       case 10:
           printf ("собаки ");
           break;
           
       case 11:
           printf ("свиньи ");
           break;
           
       
   }
return 0;
}
